{
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "colors": {
    // Lists
    "list.hoverBackground": "#FFFFFF0B",
    "list.hoverForeground": "#E8E8E8",
    "list.activeSelectionBackground": "#FFFFFF10",
    "list.activeSelectionForeground": "#E8E8E8",
    "list.activeSelectionIconForeground": "#E8E8E8",
    "list.inactiveSelectionBackground": "#FFFFFF10",
    "list.inactiveSelectionForeground": "#C0C1C2",
    "list.inactiveSelectionIconForeground": "#AFB1B3",
    // Scrollbars
    "scrollbarSlider.background": "#FFFFFF1A",
    "scrollbarSlider.hoverBackground": "#FFFFFF2A",
    "scrollbarSlider.activeBackground": "#FFFFFF30",
    "scrollbar.shadow": "#00000030",
    // Menu
    "menu.background": "#3C3F41",
    "menu.foreground": "#AFB1B3",
    "menu.separatorBackground": "#FFFFFF30",
    "menu.selectionBackground": "#1177BBCC",
    "menubar.selectionBackground": "#FFFFFF0B",
    "menubar.selectionForeground": "#E8E8E8",
    // Input Fields
    "input.background": "#FFFFFF10",
    // General Icons
    "icon.foreground": "#AFB1B3",
    // Title Bar
    "titleBar.activeBackground": "#3C3F41",
    "titleBar.activeForeground": "#AFB1B3",
    "titleBar.inactiveBackground": "#3C3F41",
    "titleBar.inactiveForeground": "#D8D8D850",
    "titleBar.border": "#00000030",
    "banner.background": "#3C3F41",
    // Tabs
    "tab.activeBackground": "#2D2F30",
    "tab.activeForeground": "#E8E8E8",
    "tab.activeBorderTop": "#1177BB",
    "tab.inactiveBackground": "#3C3F41",
    "tab.border": "#3C3F41",
    "tab.lastPinnedBorder": "#FFFFFF1A",
    "editorGroupHeader.tabsBackground": "#3C3F41",
    "editorGroupHeader.noTabsBackground": "#3C3F41",
    // Editor Group
    "editorGroup.emptyBackground": "#3C3F41",
    // Activity Bar
    "activityBar.background": "#3C3F41",
    "activityBar.foreground": "#E8E8E8",
    "activityBar.border": "#00000030",
    "activityBar.activeBackground": "#2D2F30",
    "activityBar.activeBorder": "#007FD4",
    "activityBar.activeFocusBorder": "#007FD4",
    "activityBar.dropBorder": "#AFB1B3",
    "activityBar.inactiveForeground": "#AFB1B3",
    "activityBarBadge.background": "#3C3F41",
    "activityBarBadge.foreground": "#E8E8E8",
    // Side Bar
    "sideBar.background": "#2D2F30",
    "sideBar.foreground": "#AFB1B3",
    "sideBar.border": "#00000030",
    "sideBar.dropBackground": "#FFFFFF30",
    "sideBarSectionHeader.background": "#2D2F30",
    "sideBarSectionHeader.foreground": "#C0C1C2",
    "sideBarSectionHeader.border": "#00000030",
    "sideBarTitle.foreground": "#C0C1C2",
    "badge.background": "#FFFFFF30",
    "badge.foreground": "#E8E8E8",
    // Status Bar
    "statusBar.background": "#3C3F41",
    "statusBar.foreground": "#AFB1B3",
    "statusBar.border": "#00000030",
    "statusBar.noFolderBackground": "#3C3F41",
    "statusBar.noFolderForeground": "#AFB1B3",
    "statusBar.noFolderBorder": "#00000030",
    "statusBar.debuggingBackground": "#3C3F41",
    "statusBar.debuggingForeground": "#AFB1B3",
    "statusBar.debuggingBorder": "#00000030",
    // Editor Main
    "editor.background": "#2B2B2B",
    "editor.foreground": "#A9B7C6",
    "editor.lineHighlightBackground": "#A0CCFF0B",
    "editor.selectionBackground": "#1177BB80",
    "editorIndentGuide.background": "#FFFFFF10",
    "editorIndentGuide.activeBackground": "#FFFFFF20",
    // Editor Gutter
    "editorGutter.background": "#2B2B2B",
    "editorLineNumber.foreground": "#606366",
    "editorLineNumber.activeForeground": "#A4A3A3",
    // Editor Widgets
    "widget.shadow": "#00000030",
    "editorWidget.background": "#313335",
    "editorHoverWidget.border": "#00000030",
    "editorSuggestWidget.foreground": "#C0C1C2",
    "editorSuggestWidget.border": "#00000030",
    "editorSuggestWidget.selectedBackground": "#FFFFFF1A",
    "editorSuggestWidget.selectedForeground": "#E8E8E8",
    "editorSuggestWidget.selectedIconForeground": "#C0C1C2",
    "breadcrumb.background": "#2D2F30",
    // Notebook
    "notebook.cellBorderColor": "#E8E8E8",
    "notebook.selectedCellBackground": "#AFB1B3",
    // Debug Tool Bar
    "debugToolBar.background": "#3C3F41",
    // Terminal
    "terminal.tab.activeBorder": "#35DB35",
  },
  "tokenColors": [
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "markup.bold",
        "markup.heading",
      ],
      "settings": {
        "fontStyle": "bold",
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#A5C261"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#D16969"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#6D9FC5"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#CE9178"
      }
    },
    // Metadata
    {
      "name": "general metadata",
      "scope": [
        "meta.embedded",
        "source.groovy.embedded",
        "header"
      ],
      "settings": {
        "foreground": "#A9B7C6"
      }
    },
    {
      "scope": [
        "meta.preprocessor",
        "entity.name.function.preprocessor",
        "entity.name.variable.preprocessor.symbol"
      ],
      "settings": {
        "foreground": "#93A629"
      }
    },
    {
      "scope": "meta.preprocessor.string",
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "scope": "meta.preprocessor.numeric",
      "settings": {
        "foreground": "#6D9FC5"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#A9B7C6"
      }
    },
    // Basics
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#CF4444"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#6B9555"
      }
    },
    {
      "scope": [
        "comment.block.documentation",
        "string.quoted.docstring.multi.python"
      ],
      "settings": {
        "foreground": "#6A8759"
      }
    },
    {
      "name": "strings",
      "scope": [
        "string",
        "string.value",
        "string.tag",
        "meta.embedded.assembly"
      ],
      "settings": {
        "foreground": "#CE9178"
      }
    },
    // Keywords & Operators
    {
      "name": "keyword fallback",
      "scope": "keyword",
      "settings": {
        "foreground": "#AD85C0"
      }
    },
    {
      "name": "operator fallback",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#A9B7C6"
      }
    },
    {
      "name": "control flow & special keywords",
      "scope": [
        "keyword.control",
        "source.cpp keyword.operator.new",
        "keyword.operator.delete",
        "keyword.other.using",
        "keyword.other.operator",
        "entity.name.operator",
      ],
      "settings": {
        "foreground": "#AD85C0"
      }
    },
    {
      "scope": "storage.type.function", // func declare keyword in some languages
      "settings": {
        "foreground": "#AD85C0"
      }
    },
    {
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#A5C261"
      }
    },
    {
      "name": "language-level variables: this.self",
      "scope": "variable.language",
      "settings": {
        "foreground": "#6D9FC5"
      }
    },
    {
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.alignof",
        "keyword.operator.typeid",
        "keyword.operator.alignas",
        "keyword.operator.instanceof",
        "keyword.operator.logical.python",
        "keyword.operator.wordlike",
        "keyword.operator.noexcept",
      ],
      "settings": {
        "foreground": "#AD85C0"
      }
    },
    {
      "name": "language-level types and accessibility modifiers",
      "scope": [
        "keyword.type",
        "storage.type.built-in.primitive",
        "storage.modifier",
        "storage.type.modifier",
      ],
      "settings": {
        "foreground": "#6D9FC5"
      }
    },
    {
      "name": "explicitly non-categorized keywords",
      "scope": "keyword.other",
      "settings": {
        "foreground": "#6D9FC5"
      }
    },
    {
      "name": "decorator mark", // usually the "@" symbol
      "scope": [
        "punctuation.decorator",
        "punctuation.definition.decorator",
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#A9B7C6"
      }
    },
    // Constants
    {
      "name": "language-level constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character"
      ],
      "settings": {
        "foreground": "#6D9FC5"
      }
    },
    {
      "name": "constants and enums",
      "scope": [
        "variable.other.constant",
        "variable.other.enummember",
        "support.constant"
      ],
      "settings": {
        "foreground": "#6796E6",
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#FFC66D"
      }
    },
    {
      "name": "interpolated strings",
      "scope": [
        "constant.character.format.placeholder",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.definition.interpolation.begin",
        "punctuation.definition.interpolation.end",
        "punctuation.section.embedded",
        "storage.type.string",
      ],
      "settings": {
        "foreground": "#769AA5"
      }
    },
    // Types
    {
      "name": "type declaration and references",
      "scope": [
        "meta.return-type",
        "support.class",
        "support.type",
        "entity.name.type",
        "entity.name.namespace",
        "entity.name.package",
        "entity.other.attribute",
        "entity.name.scope-resolution",
        "entity.name.class",
        "storage.type.numeric.go",
        "storage.type.byte.go",
        "storage.type.boolean.go",
        "storage.type.string.go",
        "storage.type.uintptr.go",
        "storage.type.error.go",
        "storage.type.rune.go",
        "storage.type.cs",
        "storage.type.generic.cs",
        "storage.type.modifier.cs",
        "storage.type.variable.cs",
        "storage.type.annotation.java",
        "storage.type.generic.java",
        "storage.type.java",
        "storage.type.object.array.java",
        "storage.type.primitive.array.java",
        "storage.type.primitive.java",
        "storage.type.token.java",
        "storage.type.groovy",
        "storage.type.annotation.groovy",
        "storage.type.parameters.groovy",
        "storage.type.generic.groovy",
        "storage.type.object.array.groovy",
        "storage.type.primitive.array.groovy",
        "storage.type.primitive.groovy"
      ],
      "settings": {
        "foreground": "#6BC7A8"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#A7D1E0"
      }
    },
    // Functions
    {
      "name": "functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "support.constant.handlebars",
        "source.powershell variable.other.member",
        "entity.name.operator.custom-literal"
      ],
      "settings": {
        "foreground": "#D3C68C"
      }
    },
    {
      "scope": [
        "entity.name.function.decorator",
        "meta.decorator entity.name.function",
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    // Variables
    {
      "name": "variables & parameter names",
      "scope": [
        "variable",
        "meta.definition.variable.name",
        "support.variable",
        "entity.name.variable",
        "constant.other.placeholder",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#A7D1E0"
      }
    },
    {
      "scope": [
        "support.type.vendored.property-name",
        "support.type.property-name",
        "variable.css",
        "variable.scss",
        "variable.other.less",
        "source.coffee.embedded",
        "meta.structure.dictionary.key.python"
      ],
      "settings": {
        "foreground": "#A7D1E0"
      }
    },
    // Regexp
    {
      "name": "regexp fallback",
      "scope": [
        "string.regexp",
        "constant.regexp",
      ],
      "settings": {
        "foreground": "#D16969"
      }
    },
    {
      "name": "regexp quantifiers",
      "scope": "keyword.operator.quantifier.regexp",
      "settings": {
        "foreground": "#A5C261"
      }
    },
    {
      "name": "regexp logical operators",
      "scope": [
        "keyword.operator.or.regexp",
        "keyword.operator.negation.regexp",
        "keyword.operator.disjunction.regexp",
        "keyword.control.anchor.regexp",
      ],
      "settings": {
        "foreground": "#a4eed5"
      }
    },
    {
      "name": "regexp match limiters",
      "scope": [
        "support.other.match.begin.regexp",
        "support.other.match.end.regexp"
      ],
      "settings": {
        "foreground": "#808080"
      }
    },
    {
      "name": "regexp character sets",
      "scope": [
        "constant.character.character-class.regexp",
        "constant.other.character-class.set.regexp",
        "constant.other.character-class.regexp",
        "constant.character.set.regexp",
        "punctuation.definition.character-class.regexp",
        "punctuation.character.set.begin.regexp",
        "punctuation.character.set.end.regexp",
      ],
      "settings": {
        "foreground": "#D8CD91"
      }
    },
    {
      "name": "regexp lookaround operators",
      "scope": [
        "keyword.operator.lookbehind.regexp",
        "keyword.operator.lookahead.regexp",
        "keyword.operator.lookbehind.negative.regexp",
        "keyword.operator.lookahead.negative.regexp",
      ],
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "name": "regexp match groups",
      "scope": [
        "punctuation.definition.group.regexp",
        "punctuation.definition.group.assertion.regexp"
      ],
      "settings": {
        "foreground": "#A7D1E0"
      }
    },
    {
      "name": "regexp parenthesis",
      "scope": [
        "punctuation.parenthesis.begin.regexp",
        "punctuation.parenthesis.end.regexp",
      ],
      "settings": {
        "foreground": "#769AA5"
      }
    },
    // Miscellaneous
    {
      "scope": [
        "punctuation.separator.period",
        "punctuation.separator.colon",
        "punctuation.section.function.begin",
        "punctuation.section.function.end"
      ],
      "settings": {
        "foreground": "#A9B7C6"
      }
    },
    {
      "scope": [
        "entity.name.label",
        "entity.name.tag",
      ],
      "settings": {
        "foreground": "#A7D1E0"
      }
    },
    {
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#808080"
      }
    },
    // Language-specific: Python
    {
      "scope": [
        "storage.type.format.python",
      ],
      "settings": {
        "foreground": "#A4EED5"
      }
    },
    {
      "name": "Python decorators",
      "scope": "source.python meta.function.decorator support.type",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "source.python string.quoted.binary.single",
        "source.python string.quoted.binary.single punctuation.definition.string.begin",
        "source.python string.quoted.binary.single punctuation.definition.string.end",
        "source.python string.quoted.binary.single storage.type.string"
      ],
      "settings": {
        "foreground": "#FFB6A0",
      }
    },
    // Language-specific: C#
    {
      "scope": [
        "meta.preprocessor.cs"
      ],
      "settings": {
        "foreground": "#AD85C0"
      }
    },
    {
      "scope": "string.unquoted.preprocessor.message.cs",
      "settings": {
        "foreground": "#A9B7C6"
      }
    },
    // Language-specific: Java
    {
      "name": "coloring of the Java import and package identifiers",
      "scope": [
        "storage.modifier.import.java",
        "variable.language.wildcard.java",
        "storage.modifier.package.java",
      ],
      "settings": {
        "foreground": "#A9B7C6"
      }
    },
    // Language-specific: PHP
    {
      "scope": [
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.end.php"
      ],
      "settings": {
        "foreground": "#808080"
      }
    },
    // Language-specific: JS
    {
      "scope": "storage.type.js",
      "settings": {
        "foreground": "#AD85C0"
      }
    },
    // Language-specific: TS
    {
      "scope": [
        "storage.type.ts",
        "storage.type.type.ts",
        "storage.type.class",
        "storage.type.interface.ts",
        "storage.type.enum.ts",
        "storage.type.namespace.ts",
        "storage.modifier.ts"
      ],
      "settings": {
        "foreground": "#AD85C0"
      }
    },
    {
      "name": "types declaration & references",
      "scope": [
        "meta.type.cast.expr",
        "meta.type.new.expr",
        "support.constant.math",
        "support.constant.dom",
        "support.constant.json",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#6BC7A8"
      }
    },
    // Language-specific: CSS
    {
      "name": "CSS property value",
      "scope": [
        "support.constant.property-value",
        "support.constant.font-name",
        "support.constant.media-type",
        "support.constant.media",
        "constant.other.color.rgb-value",
        "constant.other.rgb-value",
        "support.constant.color"
      ],
      "settings": {
        "foreground": "#6D9FC5"
      }
    },
    // Language-specific: Markdown
    {
      "scope": "punctuation.definition.quote.begin.markdown",
      "settings": {
        "foreground": "#AD85C0"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#6D9FC5"
      }
    },
    {
      "scope": "markup.fenced_code.block.markdown",
      "settings": {
        "foreground": "#CE9178"
      }
    },
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "newOperator": "#AD85C0",
    "stringLiteral": "#CE9178",
    "customLiteral": "#A9B7C6",
    "numberLiteral": "#6D9FC5"
  }
}