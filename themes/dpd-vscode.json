{
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "colors": {
    // Lists
    "list.hoverBackground": "#FFFFFF0B",
    "list.hoverForeground": "#E8E8E8",
    "list.activeSelectionBackground": "#FFFFFF10",
    "list.activeSelectionForeground": "#E8E8E8",
    "list.activeSelectionIconForeground": "#E8E8E8",
    "list.inactiveSelectionBackground": "#FFFFFF10",
    "list.inactiveSelectionForeground": "#C0C1C2",
    "list.inactiveSelectionIconForeground": "#AFB1B3",
    // Scrollbars
    "scrollbarSlider.background": "#FFFFFF1A",
    "scrollbarSlider.hoverBackground": "#FFFFFF2A",
    "scrollbarSlider.activeBackground": "#FFFFFF30",
    "scrollbar.shadow": "#00000030",
    // Menu
    "menu.background": "#3C3F41",
    "menu.foreground": "#AFB1B3",
    "menu.separatorBackground": "#FFFFFF30",
    "menu.selectionBackground": "#1177BBCC",
    "menubar.selectionBackground": "#FFFFFF0B",
    "menubar.selectionForeground": "#E8E8E8",
    // Input Fields
    "input.background": "#FFFFFF10",
    // General Icons
    "icon.foreground": "#AFB1B3",
    // Title Bar
    "titleBar.activeBackground": "#3C3F41",
    "titleBar.activeForeground": "#AFB1B3",
    "titleBar.inactiveBackground": "#3C3F41",
    "titleBar.inactiveForeground": "#D8D8D850",
    "titleBar.border": "#00000030",
    "banner.background": "#3C3F41",
    // Tabs
    "tab.activeBackground": "#2D2F30",
    "tab.activeForeground": "#E8E8E8",
    "tab.activeBorderTop": "#1177BB",
    "tab.inactiveBackground": "#3C3F41",
    "tab.border": "#3C3F41",
    "tab.lastPinnedBorder": "#FFFFFF1A",
    "editorGroupHeader.tabsBackground": "#1E1F22",
    "editorGroupHeader.noTabsBackground": "#3C3F41",
    // Editor Group
    "editorGroup.emptyBackground": "#3C3F41",
    // Activity Bar
    "activityBar.background": "#3C3F41",
    "activityBar.foreground": "#E8E8E8",
    "activityBar.border": "#00000030",
    "activityBar.activeBackground": "#2D2F30",
    "activityBar.activeBorder": "#007FD4",
    "activityBar.activeFocusBorder": "#007FD4",
    "activityBar.dropBorder": "#AFB1B3",
    "activityBar.inactiveForeground": "#AFB1B3",
    "activityBarBadge.background": "#3C3F41",
    "activityBarBadge.foreground": "#E8E8E8",
    // Side Bar
    "sideBar.background": "#2D2F30",
    "sideBar.foreground": "#AFB1B3",
    "sideBar.border": "#00000030",
    "sideBar.dropBackground": "#FFFFFF30",
    "sideBarSectionHeader.background": "#2D2F30",
    "sideBarSectionHeader.foreground": "#C0C1C2",
    "sideBarSectionHeader.border": "#00000030",
    "sideBarTitle.foreground": "#C0C1C2",
    "badge.background": "#FFFFFF30",
    "badge.foreground": "#E8E8E8",
    // Status Bar
    "statusBar.background": "#1E1F22",
    "statusBar.foreground": "#AFB1B3",
    "statusBar.border": "#00000030",
    "statusBar.noFolderBackground": "#3C3F41",
    "statusBar.noFolderForeground": "#AFB1B3",
    "statusBar.noFolderBorder": "#00000030",
    "statusBar.debuggingBackground": "#3C3F41",
    "statusBar.debuggingForeground": "#AFB1B3",
    "statusBar.debuggingBorder": "#00000030",
    // Editor Main
    "editor.background": "#1E1F22",
    "editor.foreground": "#A9B7C6",
    "editor.lineHighlightBackground": "#A0CCFF0B",
    "editor.selectionBackground": "#1177BB80",
    "editorIndentGuide.background": "#FFFFFF10",
    "editorIndentGuide.activeBackground": "#FFFFFF20",
    // Editor Gutter
    "editorGutter.background": "#1E1F22",
    "editorLineNumber.foreground": "#606366",
    "editorLineNumber.activeForeground": "#A4A3A3",
    // Editor Widgets
    "widget.shadow": "#00000030",
    "editorWidget.background": "#313335",
    "editorHoverWidget.border": "#00000030",
    "editorSuggestWidget.foreground": "#C0C1C2",
    "editorSuggestWidget.border": "#00000030",
    "editorSuggestWidget.selectedBackground": "#FFFFFF1A",
    "editorSuggestWidget.selectedForeground": "#E8E8E8",
    "editorSuggestWidget.selectedIconForeground": "#C0C1C2",
    "breadcrumb.background": "#2D2F30",
    // Notebook
    "notebook.cellBorderColor": "#E8E8E8",
    "notebook.selectedCellBackground": "#AFB1B3",
    // Debug Tool Bar
    "debugToolBar.background": "#3C3F41",
    // Terminal
    "terminal.tab.activeBorder": "#35DB35",
  },
  "tokenColors": [
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "markup.bold",
        "markup.heading",
      ],
      "settings": {
        "fontStyle": "bold",
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#A5C261"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#D16969"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#6897BB"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#719161"
      }
    },
    // Metadata
    {
      "name": "general metadata",
      "scope": [
        "meta.embedded",
        "source.groovy.embedded",
        "header",
      ],
      "settings": {
        "foreground": "#A9B7C6"
      }
    },
    {
      "scope": [
        "entity.name.function.preprocessor",
        "entity.name.variable.preprocessor.symbol",
      ],
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "scope": "meta.preprocessor.numeric",
      "settings": {
        "foreground": "#6897BB"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#A9B7C6"
      }
    },
    // Basics
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#CF4444"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#808080"
      }
    },
    {
      "scope": [
        "comment.block.documentation",
        "string.quoted.docstring.multi.python",
      ],
      "settings": {
        "foreground": "#629755"
      }
    },
    {
      "name": "strings",
      "scope": [
        "string",
        "string.value",
        "string.tag",
        "meta.embedded.assembly",
      ],
      "settings": {
        "foreground": "#719161"
      }
    },
    // Keywords & Operators
    {
      "name": "keyword fallback",
      "scope": [
        "keyword",
        "keyword.language",
        "keyword.other",
      ],
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "operator fallback",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#BCBEC4"
      }
    },
    {
      "name": "control flow & special keywords",
      "scope": [
        "keyword.control",
        "source.cpp keyword.operator.new",
        "keyword.operator.delete",
        "keyword.other.using",
        "keyword.other.operator",
        "entity.name.operator",
        "keyword",
        "keyword.operator.logical",
        "keyword.operator.constructor",
      ],
      "settings": {
        "foreground": "#CF8E6D"
      }
    },
    {
      "scope": [
        "storage.modifier",
        "storage.type.modifier",
        "storage.type.function", // func declare keyword in some languages
        "storage.type.var", // variable declare keyword in some languages
        "storage.type.const", // constant declare keyword in some languages
      ],
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#A5C261"
      }
    },
    {
      "name": "language-level variables: this.self",
      "scope": "variable.language",
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.alignof",
        "keyword.operator.typeid",
        "keyword.operator.alignas",
        "keyword.operator.instanceof",
        "keyword.operator.logical.python",
        "keyword.operator.wordlike",
        "keyword.operator.noexcept",
      ],
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "language-level types",
      "scope": [
        "keyword.type",
        "storage.type.built-in",
        "storage.type.built-in",
        "storage.type.built-in.primitive",
        "storage.type.basic",
        "storage.modifier",
        "storage.type.modifier",
      ],
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "explicitly non-categorized keywords",
      "scope": "keyword.other",
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "decorator mark",
      "scope": "punctuation.definition.decorator", // usually the "@" symbol
      "settings": {
        "foreground": "#A9B7C6"
      }
    },
    {
      "name": "overrides",
      "scope": [
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end",
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.definition.interpolation.begin",
        "punctuation.definition.interpolation.end",
        "storage.type.function.arrow", // arrow func in some languages
      ],
      "settings": {
        "foreground": "#A9B7C6"
      }
    },
    // Data Structure
    {
      "name": "property",
      "scope": [
        "variable.other.property",
        "entity.name.variable.field",
        "entity.name.variable.property",
        "entity.name.variable.event", // actually only used in C#
      ],
      "settings": {
        "foreground": "#9876AA"
      }
    },
    // Other Go
    {
      "scope": "variable.other.readwrite",
      "settings": {
        "foreground": "#BCBEC4",
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#BCBEC4"
      }
    },
    // Constants
    {
      "name": "language-level constants",
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
      ],
      "settings": {
        "foreground": "#6897BB"
      }
    },
    {
      "name": "constants and enums",
      "scope": [
        "variable.other.constant",
        "variable.other.enummember",
        "constant.other.enum",
        "support.constant",
      ],
      "settings": {
        "foreground": "#C77DBB"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#CC7832"
      }
    },
    // Functions
    {
      "name": "functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "support.constant.handlebars",
        "source.powershell variable.other.member",
        "entity.name.operator.custom-literal",
        "entity.name.function.go",
        "entity.name.function.member",
      ],
      "settings": {
				"foreground": "#FFC66D"
      }
    },
    // Regexp
    {
      "name": "regexp fallback",
      "scope": [
        "string.regexp",
        "constant.regexp",
      ],
      "settings": {
        "foreground": "#719161"
      }
    },
    {
      "name": "regexp quantifiers",
      "scope": "keyword.operator.quantifier.regexp",
      "settings": {
        "foreground": "#6897BB"
      }
    },
    {
      "name": "regexp logical operators",
      "scope": [
        "keyword.operator.or.regexp",
        "keyword.operator.negation.regexp",
        "keyword.operator.disjunction.regexp",
        "keyword.control.anchor.regexp",
      ],
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "regexp match limiters",
      "scope": [
        "support.other.match.begin.regexp",
        "support.other.match.end.regexp",
      ],
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "regexp lookaround operators",
      "scope": [
        "keyword.operator.lookbehind.regexp",
        "keyword.operator.lookahead.regexp",
        "keyword.operator.lookbehind.negative.regexp",
        "keyword.operator.lookahead.negative.regexp",
      ],
      "settings": {
        "foreground": "#E8BF6A"
      }
    },
    {
      "name": "regexp parenthesis",
      "scope": [
        "punctuation.parenthesis.begin.regexp",
        "punctuation.parenthesis.end.regexp",
        "punctuation.character.set.begin.regexp",
        "punctuation.character.set.end.regexp",
        "punctuation.definition.character-class.regexp",
      ],
      "settings": {
        "foreground": "#E8BF6A"
      }
    },
    // Miscellaneous
    {
      "scope": [
        "punctuation.terminator",
        "punctuation.separator.comma",
        "punctuation.separator.list.comma",
      ],
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "scope": "punctuation.separator.colon",
      "settings": {
        "foreground": "#A9B7C6"
      }
    },
    {
      "scope": "variable.argument",
      "settings": {
        "foreground": "#A9B7C6"
      }
    },
    {
      "scope": [
        "entity.name.label",
        "entity.name.tag",
      ],
      "settings": {
        "foreground": "#E8BF6A"
      }
    },
    {
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#E8BF6A"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#BABABA"
      }
    },
    // Language-specific: C++
    {
      "scope": [
        "source.cpp storage.type.extern",
        "source.cpp storage.type.struct",
        "source.cpp storage.type.union",
        "source.cpp storage.type.enum",
        "source.cpp storage.type.template",
        "source.cpp storage.type.asm",
        "source.cpp storage.type.decltype",
      ],
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "scope": "source.cpp entity.name.other.preprocessor.macro",
      "settings": {
        "foreground": "#CC7832"
      }
    },
    // Language-specific: Python
    {
      "scope": "variable.parameter.function.language.special.python",
      "settings": {
        "foreground": "#9876AA",
      }
    },
    {
      "scope": "variable.parameter.function-call.python",
      "settings": {
        "foreground": "#AF5635"
      }
    },
    {
      "scope": [
        "support.variable.magic.python",
        "support.function.magic.python",
      ],
      "settings": {
        "foreground": "#A552A5"
      }
    },
    {
      "scope": [
        "source.python support.type",
        "source.python meta.function-call.arguments support.type",
        "source.python meta.function-call support.function.builtin",
        "source.python meta.item-access meta.item-access.arguments support.type",
      ],
      "settings": {
        "foreground": "#8888C6"
      }
    },
    {
      "scope": [
        "source.python punctuation.section.function.begin",
        "source.python meta.function-call.python",
        "constant.character.format.placeholder.other.python",
      ],
      "settings": {
        "foreground": "#A9B7C6",
      }
    },
    {
      "scope": [
        "source.python string.quoted.binary.single",
        "source.python string.quoted.binary.single punctuation.definition.string.begin",
        "source.python string.quoted.binary.single punctuation.definition.string.end",
        "source.python string.quoted.binary.single storage.type.string",
      ],
      "settings": {
        "foreground": "#A5C261",
      }
    },
    {
      "scope": [
        "source.python support.type.exception",
        "source.python meta.class.inheritance support.type.exception",
        "source.python meta.class.inheritance support.type.metaclass",
      ],
      "settings": {
        "foreground": "#8888C6"
      }
    },
    // Language-specific: C#
    {
      "scope": "meta.preprocessor.cs",
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "scope": "string.unquoted.preprocessor.message.cs",
      "settings": {
        "foreground": "#A9B7C6"
      }
    },
    // Language-specific: Go
    {
      "scope": [
        "source.go entity.name.type",
        "storage.type.numeric.go",
        "storage.type.byte.go",
        "storage.type.boolean.go",
        "storage.type.string.go",
        "storage.type.uintptr.go",
        "storage.type.error.go",
        "storage.type.rune.go",
      ],
      "settings": {
        // "foreground": "#CC7832"
        "foreground": "#6EAEA8"
      }
    },
    {
      "scope": "entity.name.package.go",
      "settings": {
        "foreground": "#B3AE60"
      }
    },
    {
      "scope": "punctuation.other.comma.go",
      "settings": {
        "foreground": "#CC7832"
      }
    },
    // Language-specific: Java
    {
      "name": "coloring of the Java import and package identifiers",
      "scope": [
        "storage.modifier.import.java",
        "variable.language.wildcard.java",
        "storage.modifier.package.java",
      ],
      "settings": {
        "foreground": "#A5C261"
      }
    },
    {
      "scope": [
        "meta.tag.custom entity.name.tag.html",
        "meta.tag.other.unrecognized entity.name.tag.html",
        "entity.name.tag",
        "entity.name.tag.class.js",
      ],
      "settings": {
        "foreground": "#26BDA4"
      }
    },
    // Language-specific: HTML
    {
      "scope": [
        "meta.tag string.unquoted.html",
        "meta.tag string.quoted.single.html",
        "meta.tag string.quoted.double.html",
        "meta.tag punctuation.separator.key-value.html",
      ],
      "settings": {
        "foreground": "#A5C261"
      }
    },
    {
      "scope": [
        "meta.tag.custom entity.name.tag.html",
        "meta.tag.other.unrecognized entity.name.tag.html",
        "entity.name.tag",
        "entity.name.tag.class.js",
      ],
      "settings": {
        "foreground": "#26BDA4"
      }
    },
    // Language-specific: PHP
    {
      "scope": [
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.end.php",
      ],
      "settings": {
        "foreground": "#E8BF6A"
      }
    },
    // Language-specific: JS
    {
      "scope": "storage.type.js",
      "settings": {
        "foreground": "#CC7832"
      }
    },
    // Language-specific: TS
    {
      "scope": [
        "storage.type.ts",
        "storage.type.type.ts",
        "storage.type.class",
        "storage.type.interface.ts",
        "storage.type.enum.ts",
        "storage.type.namespace.ts",
        "storage.modifier.ts",
      ],
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "types declaration & references",
      "scope": [
        "meta.type.cast.expr",
        "meta.type.new.expr",
        "support.constant.math",
        "support.constant.dom",
        "entity.other.inherited-class",
      ],
      "settings": {
        "foreground": "#A9B7C6"
      }
    },
    // Language-specific: TSX
    {
      "scope": "storage.type.tsx",
      "settings": {
        "foreground": "#CC7832"
      }
    },
    // Language-specific: Shader languages
    {
      "name": "types declaration & references",
      "scope": [
        "storage.type.glsl",
        "storage.type.basic.hlsl",
        "storage.type.structured.hlsl",
        "storage.type.structure.shaderlab",
      ],
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "name": "helpers",
      "scope": "support.type.basic.shaderlab",
      "settings": {
        "foreground": "#DAAE67"
      }
    },
    // Language-specific: CSS
    {
      "name": "CSS selector fallback",
      "scope": "source.css entity.other.attribute-name",
      "settings": {
        "foreground": "#E8BF6A"
      }
    },
    {
      "name": "CSS class selector",
      "scope": "entity.other.attribute-name.class.css",
      "settings": {
        "foreground": "#E8BF6A"
      }
    },
    {
      "name": "CSS id selector",
      "scope": "entity.other.attribute-name.id.css",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#E8BF6A"
      }
    },
    {
      "name": "CSS pseudo selector",
      "scope": [
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css",
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#E8BF6A"
      }
    },
    {
      "name": "CSS selector operator",
      "scope": [
        "source.css meta.selector keyword.operator",
        "source.css meta.selector punctuation.definition.entity",
      ],
      "settings": {
        "foreground": "#A9B7C6"
      }
    },
    {
      "name": "CSS property name",
      "scope": "meta.property-name.css",
      "settings": {
        "foreground": "#BABABA"
      }
    },
    {
      "name": "CSS vendored property name",
      "scope": "support.type.vendored.property-name.css",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "CSS property value fallback",
      "scope": "meta.property-value.css",
      "settings": {
        "foreground": "#A9B7C6"
      }
    },
    {
      "name": "CSS RGB value",
      "scope": [
        "source.css constant.other.color.rgb-value",
        "source.css constant.other.rgb-value",
      ],
      "settings": {
        "foreground": "#6897BB"
      }
    },
    {
      "name": "CSS unicode range value",
      "scope": "constant.other.unicode-range.css",
      "settings": {
        "foreground": "#6897BB"
      }
    },
    {
      "name": "CSS URL value",
      "scope": "variable.parameter.url.css",
      "settings": {
        "foreground": "#287BDE"
      }
    },
    {
      "name": "CSS important",
      "scope": "keyword.other.important.css",
      "settings": {
        "fontStyle": "bold"
      }
    },
    // Language-specific: JSON
    {
      "name": "JSON keys",
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#9876AA"
      }
    },
    {
      "name": "JSON constants",
